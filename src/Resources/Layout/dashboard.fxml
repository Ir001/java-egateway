<AnchorPane fx:id="paneDashboard" prefHeight="480" prefWidth="640" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controller.DashboardController">
   <children>
      <TabPane layoutX="11.0" layoutY="58.0" prefHeight="408.0" prefWidth="619.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Dashboard">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView layoutX="14.0" layoutY="14.0" prefHeight="361.0" prefWidth="594.0">
                          <columns>
                            <TableColumn prefWidth="75.0" text="Air Quality" />
                            <TableColumn prefWidth="75.0" text="CO2" />
                              <TableColumn prefWidth="75.0" text="O2" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Calibration">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="510.0" layoutY="324.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="96.0" text="Save Changes" />
                        <TextField layoutX="14.0" layoutY="44.0" prefHeight="39.0" prefWidth="287.0" promptText="Lorem ipsum" />
                        <Text fill="#333333" layoutX="14.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Lorem ipsum">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Text>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <ImageView fx:id="imgProfileIcon" fitHeight="30.0" fitWidth="30.0" layoutX="599.0" layoutY="20.0" onMouseClicked="#profileClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/profile-user.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="48.0" fitWidth="51.0" layoutX="11.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/trusur.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
